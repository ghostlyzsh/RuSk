native function printf(msg: text, ...) :: int

struct addr:
    first :: int
    second :: int
    third :: int
    fourth :: int

set {var} to struct addr(first: 127, second: 0, third: 0, fourth: 1)

set {var::first} to 128
set {var::second} to 124
set {var::third} to 12

printf("%d\n\0", {var::first})
printf("%d\n\0", {var::second})
printf("%d\n\0", {var::third})
printf("%d\n\0", {var::fourth})
